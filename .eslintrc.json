{
    "env": {
        "browser": true,
        "es2021": true,
        "googleappsscript/googleappsscript": true,
        "jest": true
    },
    "parser": "@typescript-eslint/parser",
    "extends": [
        "airbnb-base",
        "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "no-console": "off",
        "class-methods-use-this": "off",
        "no-continue": "off",
        "no-restricted-syntax": "off",
        "strict": "off",
        "@typescript-eslint/triple-slash-reference": "off"
    },
    "plugins": [
        "googleappsscript",
        "jest",
        "@typescript-eslint"
    ],
    "globals": {
        "globalRegister": "readonly",
        "assert": "readonly",
        "FetchingService": "readonly",
        "FileSystemConnector": "readonly",
        "DashboardRepository": "readonly",
        "FinalizationRepository": "readonly",
        "ParsedFilesRepository": "readonly",
        "RawFilesRepository": "readonly",
        "BackpropagationService": "readonly",
        "DashboardService": "readonly",
        "ExtractService": "readonly",
        "FinalizationService": "readonly",
        "MaintenanceService": "readonly",
        "TransformService": "readonly",
        "GenshinHoneyHunterWorldParser": "readonly",
        "Helpers": "readonly",
        "LogManager": "readonly",
        "Constants": "readonly"
    },
    "root": true
}
